    <servlet-mapping>
        <servlet-name>admvalidate</servlet-name>
        <url-pattern>/admvalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admlogout</servlet-name>
        <url-pattern>/admlogout</url-pattern>
